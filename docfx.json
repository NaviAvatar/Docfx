{
  "build": {
    "content": [
      {
        "files": [
          "OperatingSystem/**.md",
          "OperatingSystem/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "CCP/**.md",
          "CCP/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "ComputerNetwork/**.md",
          "ComputerNetwork/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "Algorithm/**.md",
          "Algorithm/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "Languages/**.md",
          "Languages/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "files": [
          "Linux/**.md",
          "Linux/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],    
    "resource": [
      {
        "files": [
          "images/**",
          "Languages/**.png",
          "Algorithm/**.png",
          "Linux/**.png",
          "OperatingSystem/**.png"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadata": {
      "_enableSearch": true
      },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/ExtractSearchIndex-zh"
    ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}